<% provide(:title, '出勤中社員') %>
<div>
  <h1>出勤中社員一覧</h1>
  <table class="table table-condensed table-hover" id="table-employee">
    <thead>
    　<tr>
    　 <th>社員番号</th>
    　 <th>名前</th>
    　</tr>
  　</thead>
    <% @users.each do |user| %>
    　<tbody>
    　   <tr>
          <td><%= user.employee_number %></td>
          <td><%= user.name %></td>
        </tr>
      </tbody>
    <% end %> 
  </table>
</div>  