<h1>拠点地新規登録</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_with(model: @position, local: true) do |f| %>
      <%= f.label :"拠点番号", class: "label-signup"%>
      <%= f.number_field :place_num,  class: "form-control" %>

      <%= f.label :"拠点地",class: "label-signup" %>
      <%= f.text_field :place_name,  class: "form-control" %>

      <%= f.submit "新規作成", class: "btn btn-primary btn-block btn-signup" %>
    <% end %>
  </div>
</div>